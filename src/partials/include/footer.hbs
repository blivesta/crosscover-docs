{{#is file.meta.load_files "bundle"}}

<script src="{{file.meta.base_url}}js/{{pkg.name}}.js"></script>

{{else}}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.1.8/imagesloaded.pkgd.min.js"></script>
<script src="{{file.meta.base_url}}js/crosscover-0.3.0.js"></script>
<script>

var Docs = (function() {

  function isInit() {
  	var randomIndex = Math.floor(Math.random() * 4) * 1;
    $(".crosscover").imagesLoaded( function() {
      $(".crosscover").crosscover({
	      startIndex: randomIndex,
        animateInClass:'{{file.meta.in_class}}',
        animateOutClass:'{{file.meta.out_class}}'
      });
    });
  }

  isInit();

  return {};
})();
</script>
{{/is}}
